<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="base" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="client.java.controllers.RulesPageController" prefHeight="800.0" prefWidth="1200.0">
    <center>
        <VBox alignment="CENTER"  spacing="20.0">
            <Label fx:id="heading">
                The Rules
            </Label>
            <TextArea fx:id="rulesfield" VBox.vgrow="ALWAYS" editable="false">
               Some Rules (To be filled in)
            </TextArea>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#onBackClick" text = "Back">

            </Button>
        </VBox>
    </center>
</BorderPane>
